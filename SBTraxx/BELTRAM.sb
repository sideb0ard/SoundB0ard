let key = 8;

let playh_fn = fn(samp, pat, dr, shvel)
{
  for (let i = 0; i < 16; i++) {
    let v = 128;
    if (shvel) {
      v = 40 + rand(40);
    }
    if (pat[i] == 1) {
      note_on_at(samp, 1, i * pp, dur = dr, vel = v);
    }
  }
}

