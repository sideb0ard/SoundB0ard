let drblah_gen = gen()
{
  setup()
  {
    let bdz =  [1, 0, 0, 0,  1, 0, 0, 0,  1, 0, 0, 0,  1, 0, 0, 0];
    let durz = [50, 100, 200, 100, 400, 100, 400, 100];
    let drx = 0;
    let d = 10;
  }
  run()
  {
    let d = durz[drx];
    playh_fn(bd, bdz, d, false);
    drx = incr(drx, 0, len(durz));
  }
}

let hhblah_gen = gen()
