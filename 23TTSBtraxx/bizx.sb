
  # surgefizz
let bzzlah_gen = gen()
{
  setup()
  {
    let t = 4096;
  }
  run()
  {

    for (i = 0; i < 16; ++i) {
      74 * ( t | ( t >> 2 | t >> 16 ) );
      ++t;
      set dx:e1att (t%600) at i*pp;
    }

    let mel = combinor(bits(t), [44, 32, 33, 47, 46]);
    play_array(dx, mel);
  }
}

