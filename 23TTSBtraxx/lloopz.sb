# let maw3 = loop(dloops/maw3_2bar_mono.wav);
# let wah = loop(loops/wawloop.aif);

let wahblah_gen = gen()
{
  setup()
  {
    let nomz = [2, 4, 8, 12, 14, 15];
    let speedz = [2.3, 4.6, 16, 23, 28, 32.52];
    let nx = 0;
  }
  run()
  {
    unsolo();
    set maw3:plooplen nomz[nx];
    set wah:speed speedz[nx];
    nx = incr(nx, 0, len(nomz));

    if (count % 8 == 7) {
      solo(maw3);
      set maw3:stutter;
    }
  }
}

